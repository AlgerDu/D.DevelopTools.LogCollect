{
    "file-input": {
        "paths": [
            "E:/das-log-test/root/**/*.log"
        ],
        "ignore": [],
        "mulitline": {
            "pattern": "^(?!Info|Debug|Trace|Warn)",
            "what": "previous",
            "speciallines": 2
        }
    },
    "regex": {
        "line1": {
            "loglevel": "^(Info|Debug|Trace|Warn)",
            "logtimestamp": "[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}.[0-9]{4}",
            "threadid": "\\[([0-9]{1,6})\\]"
        },
        "line2": {
            "logcatoage": "^[^\\s,\\[]+",
            "callsiteline": "\\[([0-9]{1,6})\\]"
        },
        "path": {
            "product": "vps",
            "env": "\\\\(test|dev|pro)\\\\",
            "project": "\\\\(test|dev|pro)\\\\([\\S]+)\\\\",
            "instanceid": "[0-9]{1,3}.[0-9]{1,3}.[0-9]{1,3}.[0-9]{1,3}"
        }
    },
    "tidy-fields": {
        "combine": {
            "esindex": "product env project",
            "estype": "basefilename instanceid"
        },
        "rename": {
            "message": "logmessage"
        }
    },
    "elasticsearch-output": {
        "host": "172.17.204.227:9200",
        "index": "esindex",
        "type": "estype",
        "fields": [
            "loglevel",
            "logtimestamp",
            "threadid",
            "logcatoage",
            "callsiteline",
            "logmessage"
        ]
    }
}